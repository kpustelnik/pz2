@model MvcLab9.ViewModels.DataViewModel
Witaj @ViewData["login"]

<form asp-action="Wylogowanie">
    <input type="submit" class="btn btn-primary" value="Wyloguj" />
</form>

@ViewData["error"]

Przechowywane przez Ciebie dane:
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Zawartość</th>
            <th>Akcje</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Data)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.data</td>
            <td>
              <form asp-action="UsunDane">
                <input type="text" name="id" hidden value=@item.Id class="form-control" />
                <input type="submit" class="btn btn-primary" value="Usuń" />
              </form>
            </td>
        </tr>
    }
    </tbody>
</table>

<form asp-action="DodajDane">
    <label for="data">Dana</label>
    <input type="text" id="data" name="data" class="form-control" />
    <input type="submit" class="btn btn-primary" value="Dodaj" />
</form>